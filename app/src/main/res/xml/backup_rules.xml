<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- Include the UberMax configuration in backups -->
    <include domain="sharedpref" path="UberMaxConfig.xml" />
    
    <!-- Exclude other app data for privacy -->
    <exclude domain="sharedpref" path="." />
    <exclude domain="database" path="." />
    <exclude domain="file" path="." />
</full-backup-content> 